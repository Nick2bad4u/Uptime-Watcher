{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "language": "en",
    "version": "0.2",
    "cache": {
        "cacheFormat": "universal",
        "cacheLocation": ".cspellcache",
        "cacheStrategy": "metadata",
        "useCache": true
    },
    "dictionaries": [
        "aws",
        "bash",
        "companies",
        "cryptocurrencies",
        "css",
        "docker",
        "en-us",
        "filetypes",
        "fonts",
        "gaming-terms",
        "git",
        "google",
        "html-symbol-entities",
        "html",
        "java",
        "javascript",
        "lorem-ipsum",
        "markdown",
        "misc",
        "node",
        "npm",
        "powershell",
        "public-licenses",
        "softwareTerms",
        "typescript",
        "Uptime-Watcher Folder Dictionary",
        "Uptime-Watcher Project Dictionary",
        "Uptime-Watcher User Dictionary"
    ],
    "dictionaryDefinitions": [
        {
            "description": "Words used in the Uptime-Watcher project",
            "name": "Uptime-Watcher Project Dictionary",
            "path": "custom-words.txt",
            "scope": "workspace",
            "addWords": true
        },
        {
            "description": "Words used in the Uptime-Watcher project",
            "name": "Uptime-Watcher User Dictionary",
            "path": "custom-words.txt",
            "scope": "user",
            "addWords": true
        },
        {
            "description": "Words used in the Uptime-Watcher project",
            "name": "Uptime-Watcher Folder Dictionary",
            "path": "custom-words.txt",
            "scope": "folder",
            "addWords": true
        }
    ],
    "ignorePaths": [
        "**CHANGELOG.md",
        "dist-electron",
        "dist",
        "docs",
        "node_modules",
        "release"
    ],
    "ignoreWords": [],
    "import": [],
    "words": [],
    "allowCompoundWords": true,
    "numSuggestions": 10,
    "suggestionsTimeout": 800
}
