{
    "name": "docs-docusaurus",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "docusaurus": "npx docusaurus",
        "start": "npx docusaurus start",
        "build": "npm run generate-docs && npx docusaurus build",
        "build:fast": "npx docusaurus build",
        "generate-docs": "npx typedoc --options typedoc.config.json && node ../../scripts/fix-mdx-typedoc-unified.js",
        "generate-docs:app": "npx typedoc --options ../../typedoc.json",
        "generate-docs:electron": "npx typedoc --options ../../typedoc.electron.json",
        "swizzle": "npx docusaurus swizzle",
        "deploy": "npx docusaurus deploy",
        "clear": "npx docusaurus clear",
        "serve": "npx docusaurus serve",
        "write-translations": "npx docusaurus write-translations",
        "write-heading-ids": "npx docusaurus write-heading-ids",
        "npx typecheck": "tsc"
    },
    "dependencies": {
        "@docusaurus/core": "3.8.1",
        "@docusaurus/preset-classic": "3.8.1",
        "@mdx-js/react": "^3.0.0",
        "clsx": "^2.0.0",
        "prism-react-renderer": "^2.3.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "3.8.1",
        "@docusaurus/tsconfig": "3.8.1",
        "@docusaurus/types": "3.8.1",
        "npx typedoc": "^0.28.7",
        "npx typedoc-docusaurus-theme": "^1.4.0",
        "npx typedoc-plugin-markdown": "^4.7.1",
        "npx typescript": "~5.6.2"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 3 chrome version",
            "last 3 firefox version",
            "last 5 safari version"
        ]
    },
    "engines": {
        "node": ">=18.0"
    }
}