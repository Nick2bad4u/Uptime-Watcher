{
    "name": "docs-docusaurus",
    "version": "0.0.0",
    "private": true,
    "author": "Nick2bad4u",
    "description": "Documentation site for Uptime Watcher using Docusaurus",
    "keywords": [
        "docusaurus",
        "documentation",
        "uptime",
        "monitoring",
        "electron"
    ],
    "bugs": {
        "url": "https://github.com/Nick2bad4u/Uptime-Watcher/issues"
    },
    "scripts": {
        "docusaurus": "docusaurus",
        "start": "docusaurus start",
        "build": "npm run generate-docs && docusaurus build",
        "build:fast": "docusaurus build",
        "generate-docs": "typedoc --options typedoc.config.json && node ../../scripts/fix-mdx-typedoc-unified.js",
        "generate-docs:app": "typedoc --options ../../typedoc.json",
        "generate-docs:electron": "typedoc --options ../../typedoc.electron.json",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@docusaurus/core": "3.8.1",
        "@docusaurus/plugin-content-docs": "3.8.1",
        "@docusaurus/preset-classic": "3.8.1",
        "@docusaurus/types": "3.8.1",
        "@mdx-js/react": "^3.1.0",
        "clsx": "^2.1.1",
        "prism-react-renderer": "^2.4.1",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "@docusaurus/faster": "3.8.1"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "3.8.1",
        "@docusaurus/tsconfig": "3.8.1",
        "typedoc": "^0.28.10",
        "typedoc-docusaurus-theme": "^1.4.2",
        "typedoc-plugin-markdown": "^4.8.1",
        "typescript": "~5.9.2"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 3 chrome version",
            "last 3 firefox version",
            "last 5 safari version"
        ]
    },
    "engines": {
        "node": ">=18.0"
    }
}