{
    "$schema": "vscode://schemas/tasks",
    "tasks": [
        {
            "detail": "Starts the Vite development server for the application.",
            "group": "build",
            "icon": {
                "color": "terminal.ansiCyan",
                "id": "play"
            },
            "isBackground": true,
            "label": "Start Vite Dev Server",
            "presentation": {
                "clear": false,
                "close": false,
                "echo": true,
                "focus": false,
                "group": "vite-dev",
                "panel": "shared",
                "preserveTerminalName": false,
                "reveal": "always",
                "revealProblems": "onProblem",
                "showReuseMessage": false
            },
            "problemMatcher": {
                "applyTo": "allDocuments",
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "^\\s*VITE v\\d+",
                    "endsPattern": "^\\s*(?:➜|›)\\s+(?:Local|Network):|ready in \\d+ms"
                },
                "owner": "vite",
                "pattern": {
                    "column": 3,
                    "file": 1,
                    "line": 2,
                    "message": 5,
                    "regexp": "^(?:\\x1b\\[[0-9;]*m)*(?:\\[.*?\\]\\s*)?(?:File:\\s*)?((?:[A-Za-z]:[\\\\/]|[\\\\/])?[^:\\r\\n]+?):(\\d+):(\\d+)(?::\\s*(?:(error|warning|info)))?[:\\s-]*(.*)$",
                    "severity": 4
                },
                "source": "vite"
            },
            "promptOnClose": false,
            "runOptions": {
                "instanceLimit": 1,
                "instancePolicy": "terminateOldest"
            },
            "script": "dev",
            "type": "npm"
        },
        {
