{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "composite": false,
        "declaration": true,
        "declarationDir": ".cache/builds/test/storybook/types",
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "noEmit": true,
        "outDir": ".cache/builds/test/storybook",
        "tsBuildInfoFile": ".cache/tsconfig.storybook.tsbuildinfo",
        "types": [
            "node",
            "vite/client",
            "@storybook/react-vite",
            "storybook/actions",
            "storybook/test",
            "msw-storybook-addon"
        ]
    },
    "exclude": [
        "../node_modules",
        "../dist",
        "../electron/test",
        "../shared/test",
        "../src/test",
        "../playwright"
    ],
    "extends": "../tsconfig.json",
    "include": [
        "./**/*.ts",
        "./**/*.tsx",
        "../src/**/*.ts",
        "../src/**/*.tsx",
        "../shared/**/*.ts",
        "../shared/**/*.tsx"
    ]
}
