{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "type": "prompt",
            "segments": [
                {
                    "background": "#0077c2",
                    "foreground": "#000000",
                    "leading_diamond": "╭─",
                    "style": "diamond",
                    "template": " {{ .Name  }} {{ .Version }} ",
                    "type": "shell",
                    "properties": {
                        "mapped_shell_names": {
                            "pwsh": "PS"
                        }
                    }
                },
                {
                    "background": "#ef5350",
                    "foreground": "#FFFB38",
                    "style": "diamond",
                    "template": "<parentBackground></>  ",
                    "type": "root"
                },
                {
                    "background": "#FF9248",
                    "foreground": "#2d3436",
                    "powerline_symbol": "",
                    "style": "powerline",
                    "template": "  {{ .Path }} ",
                    "type": "path",
                    "properties": {
                        "folder_icon": "  ",
                        "folder_separator_icon": "/",
                        "home_icon": "",
                        "style": "folder",
                        "mapped_locations": {
                            "C:\\Users\\Nick\\Dropbox\\PC (2)\\Documents\\GitHub\\Uptime-Watcher": "UW"
                        },
                        "mapped_locations_enabled": true
                    }
                },
                {
                    "background": "#FFFB38",
                    "foreground": "#011627",
                    "powerline_symbol": "",
                    "style": "powerline",
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350>  {{ .Staging.String }}</>{{ end }} ",
                    "type": "git",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
                        "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
                        "{{ if gt .Behind 0 }}#C792EA{{ end }}"
                    ],
                    "properties": {
                        "branch_icon": "  ",
                        "fetch_status": false,
                        "fetch_upstream_icon": true
                    }
                },
                {
                    "background": "#83769c",
                    "foreground": "#000000",
                    "style": "diamond",
                    "template": " 󱑅 {{ .FormattedMs }}⠀",
                    "trailing_diamond": "",
                    "type": "executiontime",
                    "properties": {
                        "threshold": 0,
                        "style": "roundrock"
                    }
                },
                {
                    "background": "#890000",
                    "foreground": "#000000",
                    "style": "diamond",
                    "template": "<parentBackground></>  ",
                    "trailing_diamond": "",
                    "type": "status",
                    "background_templates": [
                        "{{ if .Error }}#890000{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": false
                    },
                    "status_template": [
                        "{{ if eq .Code 0 }}{{ else }}{{ end }}"
                    ]
                }
            ]
        },
        {
            "alignment": "right",
            "type": "prompt",
            "segments": [
                {
                    "min_width": 118,
                    "background": "#437683",
                    "foreground": "#000000",
                    "powerline_symbol": "",
                    "style": "powerline",
                    "template": " 󰇅 {{ round .Load1 .Precision  }}%  {{ round .PhysicalPercentUsed .Precision  }}%  {{ round .Disks .Precision  }}% ",
                    "type": "sysinfo",
                    "properties": {
                        "precision": 2
                    },
                    "invert_powerline": true
                },
                {
                    "min_width": 118,
                    "background": "#b2bec3",
                    "foreground": "#222222",
                    "powerline_symbol": "",
                    "style": "powerline",
                    "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
                    "type": "os",
                    "properties": {
                        "linux": "",
                        "macos": "",
                        "windows": ""
                    },
                    "invert_powerline": true
                },
                {
                    "min_width": 118,
                    "background": "#40c4ff",
                    "foreground": "#000000",
                    "style": "diamond",
                    "template": "  {{ .CurrentDate | date .Format }} ",
                    "trailing_diamond": "",
                    "type": "time",
                    "properties": {
                        "time_format": "3:04 PM"
                    }
                },
                {
                    "min_width": 118,
                    "background": "#f36943",
                    "foreground": "#262626",
                    "powerline_symbol": "",
                    "style": "powerline",
                    "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }} ",
                    "type": "battery",
                    "background_templates": [
                        "{{if eq \"Charging\" .State.String}}#b8e994{{end}}",
                        "{{if eq \"Discharging\" .State.String}}#fff34e{{end}}",
                        "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
                    ],
                    "properties": {
                        "charged_icon": " ",
                        "charging_icon": " ",
                        "discharging_icon": " "
                    },
                    "invert_powerline": true
                }
            ]
        },
        {
            "alignment": "right",
            "type": "rprompt",
            "segments": [
                {
                    "background": "#2f3b45",
                    "foreground": "#cfd8dc",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "style": "diamond",
                    "template": " ⌁ {{ .PromptCount }} ",
                    "type": "text",
                    "properties": {
                        "always_enabled": true
                    }
                }
            ]
        },
        {
            "alignment": "left",
            "type": "prompt",
            "segments": [
                {
                    "foreground": "#21c7c7",
                    "style": "plain",
                    "template": "╰─",
                    "type": "text"
                },
                {
                    "foreground": "#ffffff",
                    "style": "diamond",
                    "template": "{{ if .SSHSession }} {{ end }}{{ .UserName }} ",
                    "type": "session"
                },
                {
                    "foreground": "#e0f8ff",
                    "style": "plain",
                    "template": " ",
                    "type": "status",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}#ef5350{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": true
                    }
                },
                {
                    "background": "#FFD664",
                    "foreground": "#111111",
                    "style": "plain",
                    "type": "upgrade",
                    "properties": {
                        "cache_duration": "168h"
                    }
                }
            ],
            "newline": true
        }
