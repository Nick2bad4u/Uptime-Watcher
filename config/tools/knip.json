{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "entry": [
        "electron/main.ts",
        "electron/preload.ts",
        "vite.config.ts",
        "vitest.config.ts",
        "config/testing/vitest.electron.config.ts",
        "config/testing/vitest.shared.config.ts"
    ],
    "ignore": [
        "shared/types/monitorTypes.ts",
        "shared/types/themeConfig.ts",
        "src/types/events.ts",
        "**/*.test.ts",
        "**/*.spec.ts",
        "**/*.bench.ts",
        "**/test/**",
        "**/tests/**",
        "docs/**",
        "scripts/**",
        "benchmarks/**"
    ],
    "ignoreBinaries": [
        "repomix",
        "rimraf",
        "git-cz",
        "npm-check",
        "npm-check-updates",
        "detect-secrets",
        "deploy",
        "serve",
        "docusaurus",
        "eslint-find-rules",
        "markdownlint",
        "are-the-types-wrong",
        "grype",
        "sloc",
        "scorecard-check",
        "resolve-audit",
        "open-cli",
        "tsg",
        "jq",
        "https://www.schemastore.org/lintstagedrc.schema.json",
        "markdownlint"
    ],
    "ignoreDependencies": [
        "@shared/types",
        "@shared/utils",
        "@shared/validation",
        "validator",
        "zod",
        "@commitlint/prompt",
        "@commitlint/prompt-cli",
        "@cspell/cspell-bundled-dicts",
        "@cspell/cspell-types",
        "@dword-design/eslint-plugin-import-alias",
        "@electron/notarize",
        "@eslint-community/eslint-plugin-eslint-comments",
        "@eslint/css",
        "@eslint/eslintrc",
        "@eslint/mcp",
        "@executeautomation/database-server",
        "@jscpd/badge-reporter",
        "@microsoft/tsdoc-config",
        "@playwright/test",
        "@putout/eslint",
        "@putout/eslint-flat",
        "@secretlint/secretlint-rule-preset-recommend",
        "@stylistic/eslint-plugin",
        "@tailwindcss/forms",
        "@tailwindcss/typography",
        "@types/async",
        "@types/cssnano",
        "@types/eslint",
        "@types/eslint-plugin-prettier",
        "@types/eslint-plugin-react-refresh",
        "@types/json-schema",
        "@types/lint-staged",
        "@types/madge",
        "@types/mocha",
        "@types/prop-types",
        "@types/react-refresh",
        "@types/semver",
        "@types/sqlite3",
        "@types/trusted-types",
        "@types/validator",
        "@types/wait-on",
        "@types/webpack",
        "@typescript-eslint/types",
        "@typescript/native-preview",
        "@vitest/coverage-istanbul",
        "@vitest/snapshot",
        "async",
        "cross-env",
        "cssnano",
        "cssnano-preset-advanced",
        "docusaurus-plugin-typedoc",
        "electron-builder-squirrel-windows",
        "electron-devtools-installer",
        "electron-publish",
        "eslint-plugin-es-x",
        "eslint-plugin-fsecond",
        "eslint-plugin-html",
        "eslint-plugin-publint",
        "globals-vitest",
        "markdownlint",
        "msw",
        "nyc",
        "pkg-types",
        "postcss-combine-duplicated-selectors",
        "postcss-sort-media-queries",
        "prettier-eslint",
        "react-refresh",
        "secretlint",
        "stylelint-config-alphabetical-order",
        "stylelint-config-recommended",
        "stylelint-react-native",
        "tailwind-scrollbar-hide",
        "ts-morph",
        "ts-morph-helpers",
        "tslib",
        "tw-animate-css",
        "type-fest",
        "typedoc-plugin-extras",
        "typedoc-docusaurus-theme",
        "typescript-eslint",
        "typescript-eslint-language-service",
        "typestat",
        "vite-bundle-visualizer",
        "vite-plugin-checker",
        "chartjs-adapter-date-fns",
        "chartjs-plugin-zoom",
        "electron-log",
        "react-icons",
        "zustand",
        "eslint-config-eslint.config.mjs",
        "@secretlint/secretlint-rule-no-dotenv",
        "@secretlint/secretlint-rule-no-homedir",
        "@secretlint/secretlint-rule-pattern",
        "@secretlint/secretlint-rule-secp256k1-privatekey"
    ],
    "ignoreExportsUsedInFile": {
        "interface": true,
        "type": true
    },
    "includeEntryExports": false,
    "project": [
        "electron/**/*.{js,ts}",
        "shared/**/*.{js,ts}",
        "!**/*.d.ts",
        "!**/node_modules/**",
        "!**/dist/**",
        "!**/dist-electron/**",
        "!**/coverage/**"
    ],
    "rules": {
        "classMembers": "warn",
        "dependencies": "error",
        "devDependencies": "error",
        "duplicates": "error",
        "enumMembers": "warn",
        "exports": "warn",
        "files": "error",
        "nsExports": "warn",
        "nsTypes": "warn",
        "types": "warn",
        "unlisted": "error"
    },
    "workspaces": {
        ".": {
            "entry": [],
            "project": []
        },
        "electron": {
            "entry": [
                "electron/main.ts",
                "electron/preload.ts"
            ],
            "project": [
                "electron/**/*.{js,ts}",
                "!electron/**/*.test.ts",
                "!electron/**/*.spec.ts"
            ]
        },
        "shared": {
            "entry": [
                "shared/index.ts"
            ],
            "project": [
                "shared/**/*.{js,ts}",
                "!shared/**/*.test.ts",
                "!shared/**/*.spec.ts"
            ]
        }
    }
}
