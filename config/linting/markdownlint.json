{
    "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/main/schema/markdownlint-config-schema.json",
    "MD003": {
        "style": "atx"
    },
    "MD007": {
        "indent": 2,
        "start_indented": false
    },
    "MD009": {
        "br_spaces": 2,
        "list_item_empty_lines": false,
        "strict": false
    },
    "MD013": false,
    "MD022": {
        "lines_above": 1,
        "lines_below": 1
    },
    "MD024": {
        "siblings_only": true
    },
    "MD025": {
        "front_matter_title": "^\\s*title\\s*[:=]",
        "level": 1
    },
    "MD029": {
        "style": "one_or_ordered"
    },
    "MD030": {
        "ol_multi": 1,
        "ol_single": 1,
        "ul_multi": 1,
        "ul_single": 1
    },
    "MD033": {
        "allowed_elements": [
            "a",
            "code",
            "pre",
            "div",
            "span",
            "img",
            "em",
            "strong",
            "style",
            "br",
            "details",
            "summary",
            "table",
            "thead",
            "tbody",
            "tr",
            "th",
            "td"
        ]
    },
    "MD040": {
        "allowed_languages": [],
        "language_only": false
    },
    "MD041": {
        "front_matter_title": "^\\s*title\\s*[:=]",
        "level": 1
    },
    "MD046": {
        "style": "consistent"
    },
    "MD048": {
        "style": "backtick"
    },
    "MD049": {
        "style": "asterisk"
    },
    "MD050": {
        "style": "asterisk"
    },
    "default": true
}
