{
    "$schema": "https://www.schemastore.org/jscpd.json",
    "gitignore": true,
    "ignore": [
        "**/node_modules/**",
        "**/.git/**",
        "**/.github/**",
        "**/dist/**",
        "**/build/**",
        "**/coverage/**",
        "**/report/**",
        "**/docs/**",
        "**/html/**",
        "**/assets/**",
        "**/icons/**",
        "**/release/**",
        "**/benchmarks/**",
        "**/scripts/**",
        "**/libs/**",
        "**/test/**",
        "**/tests/**",
        "**/src/test/**",
        "**/electron/test/**",
        "**/vscode/**",
        "**/*.d.ts",
        "**/*.test.ts",
        "**/*.spec.ts",
        "**/*.test.tsx",
        "**/*.spec.tsx",
        "**/*.config.ts",
        "**/*.config.js",
        "**/*.config.mjs",
        "**/*.json",
        "**/*.md",
        "**/*.xml",
        "**/*.html",
        "**/*.css",
        "**/*.scss",
        "**/*.sass",
        "**/*.svg",
        "**/*.png",
        "**/*.jpg",
        "**/*.jpeg",
        "**/*.gif",
        "**/*.ico",
        "**/*.woff",
        "**/*.woff2",
        "**/*.ttf",
        "**/*.eot",
        "**/*.zip",
        "**/*.tar",
        "**/*.gz",
        "**/*.bin",
        "**/*.wasm"
    ],
    "maxSize": "100kb",
    "minLines": 10,
    "minTokens": 50,
    "mode": "mild",
    "output": "./reports/jscpd/",
    "reporters": [
        "html",
        "markdown",
        "json",
        "console",
        "xml",
        "csv"
    ],
    "threshold": 50
}
