{
    "$schema": "https://www.schemastore.org/tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "baseUrl": "../..",
        "checkJs": true,
        "composite": true,
        "declaration": true,
        "declarationDir": "../../.cache/builds/test/js/types",
        "incremental": true,
        "isolatedDeclarations": false,
        "noEmit": false,
        "noImplicitAny": false,
        "noPropertyAccessFromIndexSignature": false,
        "noUncheckedIndexedAccess": false,
        "outDir": "../../.cache/builds/test/js",
        "paths": {
            "@app/*": [
                "src/*"
            ],
            "@assets/*": [
                "assets/*"
            ],
            "@electron/*": [
                "electron/*"
            ],
            "@shared/*": [
                "shared/*"
            ],
            "eslint-plugin-comment-length": [
                "config/testing/types/eslint-plugin-comment-length.d.ts"
            ],
            "eslint-plugin-total-functions": [
                "config/testing/types/eslint-plugin-total-functions.d.ts"
            ]
        },
        "rootDir": "../..",
        "tsBuildInfoFile": "../../.cache/tsbuildinfo/tsconfig.js.tsbuildinfo"
    },
    "exclude": [
        "../../electron/**/*.test.ts",
        "../../electron/**/*.test.tsx",
        "../../**/docs/docusaurus",
        "**/node_modules/**",
        "../../node_modules/**"
    ],
    "extends": "../../tsconfig.json",
    "include": [
        "../../*.config.js",
        "../../*.config.mjs",
        "../../**/*.config.js",
        "../../**/*.config.mjs",
        "../../eslint.config.mjs",
        "../../postcss.config.js",
        "../../tailwind.config.mjs",
        "../../scripts/*.mjs",
        "../../scripts/*.cjs",
        "../../scripts/*.js",
        "../../config/linting/**/**.mjs",
        "../../playwright/codegen-template.mjs"
    ],
    "references": [
        {
            "path": "../../tsconfig.shared.json"
        }
    ]
}
