{
    "$schema": "https://www.schemastore.org/tsconfig.json",
    "compilerOptions": {
        "baseUrl": "../..",
        "composite": true,
        "declaration": true,
        "declarationDir": ".cache/builds/configs/types",
        "incremental": true,
        "isolatedDeclarations": false,
        "noEmit": false,
        "outDir": "../../.cache/builds/configs",
        "paths": {
            "@app/*": [".cache/builds/app/types/src/*"],
            "@electron/*": [".cache/builds/app/types/electron/*"],
            "@shared/*": [".cache/builds/app/types/shared/*"]
        },
        "rootDir": "../..",
        "tsBuildInfoFile": "../../.cache/tsconfig.configs.tsbuildinfo"
    },
    "exclude": [
        "node_modules",
        "**/.cache/builds/configs/**/*.d.ts",
        "**/config/tools/knip.config.ts",
        "../../config/tools/knip.config.ts",
        "config/tools/knip.config.ts"
    ],
    "extends": "../../tsconfig.json",
    "include": [
        "../../*.config.js",
        "../../*.config.mjs",
        "../../*.config.ts",
        "../../**.config.ts",
        "../../**/*.config.js",
        "../../**/*.config.mjs",
        "../../**/*.config.ts",
        "../../eslint.config.mjs",
        "../../postcss.config.js",
        "../../tailwind.config.mjs",
        "**/tailwind.config.mjs**/",
        "**.config.js",
        "**.config.mjs",
        "eslint.config.mjs",
        "postcss.config.mjs",
        "stylelint.config.mjs",
        "tailwind.config.mjs",
        "../../vitest.config.ts",
        "vitest.config.ts",
        "vitest.electron.config.ts",
        "../../vitest.electron.config.ts",
        "../../vitest.storybook.config.ts",
        "**/vitest.shared.config.ts",
        "**/vitest.stryker.config.ts",
        "../../vitest.shared.config.ts",
        "../../vitest.stryker.config.ts",
        "../../vite.playwright-coverage.config.ts",
        "docs/docusaurus/stylelint.config.mjs",
        "**/playwright.config.ts",
        "../../playwright/utils/createPlaywrightCoveragePlugin.ts",
        "../../storybook/viteSharedConfig.ts",
        "../../storybook/**/*.ts",
        "../../storybook/**/*.tsx",
        "../../vitest.config.ts",
        "../../vitest.shared.config.ts",
        "../../vitest.storybook.config.ts",
        "../../storybook/vitest.setup.ts",
        "../../vite.config.ts",
        "../../tailwind.config.mjs",
        "../../electron-builder.config.ts",
        "storybook/viteSharedConfig",
        "vite/client"
    ],
    "references": [
        {
            "path": "../../tsconfig.shared.json"
        },
        {
            "path": "../../tsconfig.electron.json"
        },
        {
            "path": "../../tsconfig.json"
        }
    ]
}
