{
    "$id": "https://github.com/nick2bad4u/uptime-watcher/blob/main/config/schemas/devin-wiki.schema.json#",
    "$schema": "https://json-schema.org/draft-07/schema#",
    "properties": {
        "pages": {
            "items": {
                "properties": {
                    "page_notes": {
                        "items": {
                            "properties": {
                                "author": {
                                    "type": "string"
                                },
                                "content": {
                                    "maxLength": 10000,
                                    "type": "string"
                                }
                            },
                            "required": ["content"],
                            "type": "object"
                        },
                        "type": "array"
                    },
                    "parent": {
                        "type": "string"
                    },
                    "purpose": {
                        "type": "string"
                    },
                    "title": {
                        "minLength": 1,
                        "type": "string"
                    }
                },
                "required": ["title", "purpose"],
                "type": "object"
            },
            "maxItems": 30,
            "type": "array"
        },
        "repo_notes": {
            "items": {
                "properties": {
                    "author": {
                        "type": "string"
                    },
                    "content": {
                        "maxLength": 10000,
                        "type": "string"
                    }
                },
                "required": ["content"],
                "type": "object"
            },
            "maxItems": 100,
            "type": "array"
        }
    },
    "required": ["repo_notes"],
    "title": "DeepWiki Schema",
    "type": "object"
}
