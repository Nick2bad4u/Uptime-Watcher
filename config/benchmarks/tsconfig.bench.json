{
    "$schema": "https://www.schemastore.org/tsconfig.json",
    "compilerOptions": {
        "baseUrl": "../..",
        "composite": true,
        "declaration": true,
        "declarationDir": "../../.cache/builds/test/bench/types",
        "exactOptionalPropertyTypes": false,
        "incremental": true,
        "isolatedDeclarations": false,
        "noImplicitAny": false,
        "noPropertyAccessFromIndexSignature": false,
        "noUncheckedIndexedAccess": false,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "outDir": "../../.cache/builds/test/bench",
        "paths": {
            "@app/*": [
                "src/*"
            ],
            "@electron/*": [
                "electron/*"
            ],
            "@shared/*": [
                "shared/*"
            ]
        },
        "rootDir": "../..",
        "strictNullChecks": true,
        "strictPropertyInitialization": false,
        "tsBuildInfoFile": "../../.cache/tsbuildinfo/tsconfig.bench.tsbuildinfo",
        "useUnknownInCatchVariables": false
    },
    "exclude": [
        "node_modules",
        "dist",
        "dist-*",
        "coverage",
        "../../**/*.test.*",
        "../../**/*.spec.*",
        "../../**/*.playwright.*",
        "../../src/test/**",
        "../../shared/test/**",
        "../../electron/test/**",
        "../../tests/**",
        "../../playwright/**",
        "**/*.d.ts",
        "**/*.js",
        "**/*.mjs"
    ],
    "extends": "../../tsconfig.shared.json",
    "include": [
        "../../benchmarks/**/*.ts",
        "../../benchmarks/**/*.mts",
        "../../benchmarks/**/*.cts",
        "../../benchmarks/**/*.tsx",
        "../../benchmarks/**/*.bench.ts",
        "../../shared/**/*.ts",
        "../../shared/**/*.mts",
        "../../shared/**/*.cts",
        "../../shared/**/*.tsx",
        "../../electron/**/*.ts",
        "../../electron/**/*.mts",
        "../../electron/**/*.cts",
        "../../electron/**/*.tsx",
        "../../src/**/*.ts",
        "../../src/**/*.mts",
        "../../src/**/*.cts",
        "../../src/**/*.tsx"
    ],
    "ts-node": {
        "esm": true,
        "experimentalSpecifierResolution": "node"
    }
}
