{
    "$schema": "https://www.schemastore.org/tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": "../",
        "checkJs": true,
        "declaration": true,
        "declarationDir": ".cache/builds/test/playwright/types",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedDeclarations": false,
        "isolatedModules": true,
        "module": "ESNext",
        "moduleResolution": "bundler",
        "noEmit": false,
        "outDir": "../.cache/builds/test/playwright",
        "paths": {
            "@/*": [
                ".cache/builds/app/types/src/*"
            ],
            "@/electron/*": [
                ".cache/builds/app/types/electron/*"
            ],
            "@/playwright/*": [
                "playwright/*"
            ],
            "@/shared/*": [
                ".cache/builds/app/types/shared/*"
            ],
            "@app/*": [
                ".cache/builds/app/types/src/*"
            ],
            "@assets/*": [
                "assets/*"
            ],
            "@electron/*": [
                ".cache/builds/app/types/electron/*"
            ],
            "@shared/*": [
                ".cache/builds/app/types/shared/*"
            ]
        },
        "pretty": true,
        "resolveJsonModule": true,
        "rootDir": "../",
        "skipLibCheck": false,
        "strict": true,
        "target": "ES2024",
        "tsBuildInfoFile": "../.cache/tsconfig.playwright.tsbuildinfo",
        "types": [
            "@playwright/test",
            "node"
        ]
    },
    "exclude": [
        "coverage",
        "node_modules",
        "test-results",
        "reports/**",
        "../.cache/builds/dist-playwright",
        "../coverage/**",
        "../node_modules/**"
    ],
    "extends": "../tsconfig.json",
    "include": [
        "**/*.ts",
        "**/*.tsx",
        "**/*.js",
        "**/*.mjs",
        "**/*.cjs",
        "fixtures/**/*.ts",
        "fixtures/**/*.tsx",
        "fixtures/**/*.js",
        "fixtures/**/*.mjs",
        "fixtures/**/*.cjs",
        "**/codegen-template.mjs"
    ],
    "references": [
        {
            "path": "../tsconfig.json"
        },
        {
            "path": "../tsconfig.electron.json"
        },
        {
            "path": "../tsconfig.shared.json"
        }
    ]
}
