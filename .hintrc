{
  "$schema": "https://raw.githubusercontent.com/webhintio/hint/refs/heads/main/packages/hint/src/lib/config/config-schema.json",
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not firefox <= 115",
    "not ios_saf <= 15.8"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ]
  }
}