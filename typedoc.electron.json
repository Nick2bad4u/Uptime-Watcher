{
    "$schema": "https://typedoc.org/schema.json",
    "name": "Uptime Watcher - Electron Main Process Documentation",
    "docsPath": "docs/docusaurus/docs/electron",
    "out": "docs/docusaurus/docs/electron",
    "tsconfig": "tsconfig.electron.json",
    "entryPoints": [
        "electron/**/*.*",
        "shared/**/*.*"
    ],
    "entryPointStrategy": "expand",
    "exclude": [
        "**/dist",
        "**/dist/**",
        "**/dist-electron",
        "**/dist-electron/**",
        "**/node_modules",
        "**/node_modules/**",
        "**/coverage",
        "**/coverage/**",
        "**/build",
        "**/build/**",
        "**/out",
        "**/out/**",
        "**/tmp",
        "**/tmp/**",
        "**/test/**",
        "**/tests/**",
        "**/spec/**",
        "**/specs/**",
        "**/examples",
        "**/examples/**",
        "**/*.test.*",
        "**/*.spec.*",
        "**/mock*",
        "**/test-*"
    ],
    "externalSymbolLinkMappings": {
        "electron": {
            "BrowserWindow": "https://www.electronjs.org/docs/latest/api/browser-window",
            "app": "https://www.electronjs.org/docs/latest/api/app",
            "ipcMain": "https://www.electronjs.org/docs/latest/api/ipc-main",
            "Menu": "https://www.electronjs.org/docs/latest/api/menu",
            "Tray": "https://www.electronjs.org/docs/latest/api/tray",
            "Notification": "https://www.electronjs.org/docs/latest/api/notification"
        },
        "node": {
            "fs": "https://nodejs.org/docs/latest/api/fs.html",
            "path": "https://nodejs.org/docs/latest/api/path.html",
            "os": "https://nodejs.org/docs/latest/api/os.html",
            "events": "https://nodejs.org/docs/latest/api/events.html"
        },
        "sqlite": {
            "Database": "https://github.com/WiseLibs/better-sqlite3/blob/master/docs/api.md#class-database"
        }
    },
    "plugin": [
        "typedoc-plugin-markdown",
        "typedoc-docusaurus-theme",
        "typedoc-plugin-mdn-links",
        "typedoc-plugin-zod",
        "typedoc-plugin-dt-links",
        "typedoc-plugin-external-package-links",
        "typedoc-plugin-coverage",
        "typedoc-plugin-missing-exports",
        "typedoc-plugin-replace-text",
        "typedoc-plugin-rename-defaults"
    ],
    "pretty": true,
    "cleanOutputDir": true,
    "readme": "README.md",
    "includeVersion": true,
    "sort": ["source-order", "alphabetical"],
    "kindSortOrder": [
        "Document",
        "Project",
        "Module",
        "Namespace",
        "Enum",
        "EnumMember",
        "Class",
        "Interface",
        "TypeAlias",
        "Constructor",
        "Property",
        "Variable",
        "Function",
        "Method",
        "Parameter",
        "TypeParameter",
        "Accessor",
        "GetSignature",
        "SetSignature",
        "CallSignature",
        "IndexSignature"
    ],
    "categoryOrder": [
        "Core",
        "Managers",
        "Services",
        "Database",
        "Events",
        "Utils",
        "Types",
        "Constants",
        "*"
    ],
    "searchInComments": true,
    "searchInDocuments": true,
    "validation": {
        "notDocumented": true,
        "invalidLink": true,
        "notExported": false
    },
    "treatWarningsAsErrors": false,
    "commentStyle": "jsdoc",
    "visibilityFilters": {
        "protected": false,
        "private": false,
        "inherited": true,
        "external": false,
        "@alpha": false,
        "@beta": false
    },
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeInternal": true,
    "excludeNotDocumented": false,
    "excludeReferences": false,
    "githubPages": true,
    "gitRevision": "main",
    "gitRemote": "origin",
    "hideGenerator": false,
    "searchGroupBoosts": {
        "Classes": 1.5,
        "Interfaces": 1.3,
        "Functions": 1.2,
        "Types": 1.1
    },
    "replaceText": {
        "inCodeCommentText": true,
        "inCodeCommentTags": true,
        "inIncludedFiles": true,
        "replacements": [
            {
                "pattern": "TODO:",
                "replace": "**TODO:**"
            },
            {
                "pattern": "FIXME:",
                "replace": "**FIXME:**"
            },
            {
                "pattern": "NOTE:",
                "replace": "**NOTE:**"
            }
        ]
    }
}
